function max(x){
    val=0;
    max_i=0;
    for(i=0;i<NR;i++){    
        if(val<=x[i]){
            val=x[i];
            max_i=i;
        }
    }
    return max_i;
}

function min(x){
    val=max(x);
    min_i=0;
    for(i=0;i<NR;i++){
        if(val>=x[i]){
            val=x[i];
            min_i=i;
        }
    }
    return min_i;   
}

BEGIN{
    index_a=0;
}

{
    a[index_a++]=$2;
    next;
}

END{
    minimum=min(a);
    maximum=max(a);
    print maximum;
}